                    suDishSalesPage.find('.date').html(start.split('-')[0] + '.' + start.split('-')[1] + '.' + start.split('-')[2] + '' + '——' + end.split('-')[0] + '.' + end.split('-')[1] + '.' + end.split('-')[2] + '');
