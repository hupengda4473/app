                chartJS.refreshTodayBusinessData(function(){
                    chartJS.refreshTodayBusinessPage();
                });
            });
            //标记错单
            $('#markWrongListPage').find('.kdsBtn').dateTime({theme:"date"},function(data,fun){
                if(fun){
                    fun();
                }
                var markWrongListPage = $('#markWrongListPage');
                markWrongListPage.find('.kdsBtn').data('time',data);
                markWrongListPage.find('.kdsBtn span').html(data);
                markWrongListPage.find('.captionDate').html(data);
                var timeStr = new Date().getTime();
                markingWrongOrdersTime = timeStr;
                markWrongListPage.find('.conList .waitPopupLayer').remove();
                markWrongListPage.find('.conList').append($('<div class="waitPopupLayer" style="display: block;">\
            <img src="images/waitLogo.gif" alt="wait" class="waitLogo" />\
            <p class="wait language" data-language="loading">正在努力加载中···</p>\
        </div>'));
                orderJS.refreshTransactionsListData(function () {
                    if (markingWrongOrdersTime !== timeStr) {
                        return
                    }
                    markWrongListPage.find('.conList .waitPopupLayer').remove();
                    orderJS.refreshMarkWrongListPage();
                });
            });
            //昨日流水
            $('#yesterdayTransPage').find('.dataBtn').dateTime({theme:"date"},function(data,fun){
                if(fun){
                    fun();
                }
                var yesterdayTransPage = $('#yesterdayTransPage');
                yesterdayTransPage.find('.dataBtn').data('time',data);
                yesterdayTransPage.find('.dataBtn span').html(data);
                yesterdayTransPage.find('.date').html(data.split('-')[0] + '.' + data.split('-')[1] + '.' + data.split('-')[2]);
                chartJS.showYesterdayChart(data, data);
            });
            //异常订单
            $('#subAbnormalOrderPage').find('.dataBtn').date({theme:"datetime",showFuture:true},function(data){
                var start = data.split(' ')[0];
                var end = data.split(' ')[1];
                var subAbnormalOrderPage = $('#subAbnormalOrderPage');
                subAbnormalOrderPage.find('.dataBtn').data('start',start).data('end',end);
                var i = 1;
                if(new Date(end).getTime() >= new Date(start).getTime()){
                    subAbnormalOrderPage.find('.date').html(start.split('-')[0] + '.' + start.split('-')[1] + '.' + start.split('-')[2] + '' + '——' + end.split('-')[0] + '.' + end.split('-')[1] + '.' + end.split('-')[2] + '');
                    $('.waitPopupLayer').show();
                    orderJS.refreshSubAbnormalOrderPage(function(){
                        $('.waitPopupLayer').hide();
                    });
                }else{
                    var errorPopupLayer = $('.errorPopupLayer');
                    errorPopupLayer.find('.title').html('<span class="language" data-language="greaterThan">结束时间应大于开始时间</span>');
                    errorPopupLayer.fadeIn(0);
                    Language.setLanguage(errorPopupLayer);
                }
            });
        });
