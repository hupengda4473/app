            //流水
            $('#subTransPage').find('.dataBtn').dateRtx({theme:"datetime",showFuture:true},function(data){
//                console.log(data);
                var start = data.split('-')[0];
                var end = data.split('-')[1];
                var subTransPage = $('#subTransPage');
                subTransPage.find('.chartBox').html('');
                subTransPage.find('.dataBtn').data('start',start).data('end',end);
                subTransPage.find('.transTop .tit').html(data.split('-')[0] + '.' + data.split('-')[1]);
                // var time = new Date(data.replace(/-/g,'/') + '/01').getTime();//选择该月份一号的时间戳
                var time = new Date(start,end - 1,1).getTime();//选择该月份一号的时间戳
                var sDate = new Date(time);
                var sDay = sDate.getDay();
                if(sDay==2 || sDay==3 || sDay==4 || sDay==5 || sDay==6){
                    sDate.setDate(sDate.getDate() - (sDay-1));
                }else if(sDay == 0){
                    sDate.setDate(sDate.getDate() - 6);
                }
//                console.log('第一天',sDate.getTime());
                var final = data.split('-')[0] +'-'+ String(Number(data.split('-')[1])+1);
                var fTime = new Date(final.replace(/-/g,'/') + '/01').getTime();//选择下个月份一号的时间戳
                var fDate = new Date(fTime);
                fDate.setDate(fDate.getDate()-1);
                var fDay = fDate.getDay();
                if(fDay==1 ||fDay==2 || fDay==3 || fDay==4 || fDay==5 || fDay==6){
                    fDate.setDate(fDate.getDate() + (7-fDay));
                }
                console.log('最后一天',fDate);
                var week = (((fDate.getTime() - sDate.getTime())/ (1000*60*60*24)) + 1)/7;
                console.log('周数',week);
                var start = new Date(sDate.getTime());
                fDate.setDate(fDate.getDate() + 1);
                var end = new Date(fDate.getTime());
                //$('.waitPopupLayer').css('top','208px').fadeIn(0);
                subTransPage.find('.transCont .waitPopupLayer').remove();
                subTransPage.find('.transCont').append($('<div class="waitPopupLayer" style="display: block;">\
            <img src="images/waitLogo.gif" alt="wait" class="waitLogo" />\
            <p class="wait language" data-language="loading">正在努力加载中···</p>\
        </div>'));
                chartJS.showChart(timeJS.getYmdTime(start),timeJS.getYmdTime(end),sDate,week);
            });
            //流水
            $('#weekTurnoverPage').find('.dataBtn').dateRtx({theme:"datetime",showFuture:true},function(data){
                console.log(data);
                var start = data.split('-')[0];
                var end = data.split('-')[1];
                var weekTurnoverPage = $('#weekTurnoverPage');
                console.log(start);
                console.log(end);
                weekTurnoverPage.find('.dataBtn').data('start',start).data('end',end);
                // var time = new Date(data.replace(/-/g,'/') + '/01').getTime();//选择该月份一号的时间戳
                var time = new Date(start,end - 1,1).getTime();//选择该月份一号的时间戳
                var sDate = new Date(time);
                var sDay = sDate.getDay();
                if(sDay==2 || sDay==3 || sDay==4 || sDay==5 || sDay==6){
                    sDate.setDate(sDate.getDate() - (sDay-1));
                }else if(sDay == 0){
                    sDate.setDate(sDate.getDate() - 6);
                }
//                console.log('第一天',sDate.getTime());
                var final = data.split('-')[0] +'-'+ String(Number(data.split('-')[1])+1);
                var fTime = new Date(final.replace(/-/g,'/') + '/01').getTime();//选择下个月份一号的时间戳
                var fDate = new Date(fTime);
                fDate.setDate(fDate.getDate()-1);
                var fDay = fDate.getDay();
                if(fDay==1 ||fDay==2 || fDay==3 || fDay==4 || fDay==5 || fDay==6){
                    fDate.setDate(fDate.getDate() + (7-fDay));
                }
                var week = (((fDate.getTime() - sDate.getTime())/ (1000*60*60*24)) + 1)/7;
                var start = new Date(sDate.getTime());
                fDate.setDate(fDate.getDate() + 1);
                var end = new Date(fDate.getTime());
                //$('.waitPopupLayer').css('top','208px').fadeIn(0);
                weekTurnoverPage.find('.markWrongListSurePageCont .waitPopupLayer').remove();
                weekTurnoverPage.find('.markWrongListSurePageCont').append($('<div class="waitPopupLayer" style="display: block;">\
            <img src="images/waitLogo.gif" alt="wait" class="waitLogo" />\
            <p class="wait language" data-language="loading">正在努力加载中···</p>\
        </div>'));
                chartJS.showChart_week(timeJS.getYmdTime(start),timeJS.getYmdTime(end),sDate,week);
            });

            //原料核算，开始时间
