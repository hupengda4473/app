<script>
    /**
     * 程序设置 开始
     * */
    //获得url中参数的方法
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]); return null;
    }
    /**
     * 程序设置 开始
     * */
    var webTest = (getQueryString('schema') != 'cordova');//web调试
    var debug = true;
    /**
     * 程序设置 结束
     * */
    /**
     * ——————————————————————————————————————————————————
     * */
    if(webTest){
        bossJS.init();
        //bossJS.testInit();
    }else{
        var showUpdatePromptLayer = function(str,fun){
            var waitPopupLayer = $('.waitPopupLayer');
            waitPopupLayer.fadeOut();
            var updatePromptLayer = $('.updatePromptLayer');
            updatePromptLayer.find('.cont').html(str);
            updatePromptLayer.fadeIn();
            var h = updatePromptLayer.find('.errorBox').height();
            updatePromptLayer.find('.errorBox').css('margin-top','-' + h/2 + 'px');
            updatePromptLayer.find('.single_btn').off('click').on('click',function(){
                waitPopupLayer.find('.remind').html('正在更新。。。');
                waitPopupLayer.fadeIn();
                updatePromptLayer.fadeOut();
                /*setTimeout(function(){
                 if(fun){
                 fun();
                 }
                 },5000);*/
                if(fun){
                    fun();
                }
            });
        };
        var showSpecialUpdatePromptLayer = function(str,fun){
            var waitPopupLayer = $('.waitPopupLayer');
            waitPopupLayer.fadeOut();
            var specialUpdatePromptLayer = $('.specialUpdatePromptLayer');
            specialUpdatePromptLayer.find('.cont').html(str);
            specialUpdatePromptLayer.fadeIn();
            var h = specialUpdatePromptLayer.find('.errorBox').height();
            specialUpdatePromptLayer.find('.errorBox').css('margin-top','-' + h/2 + 'px');
            specialUpdatePromptLayer.find('.true').off('click').on('click',function(){
                waitPopupLayer.find('.remind').html('正在更新。。。');
                waitPopupLayer.fadeIn();
                specialUpdatePromptLayer.fadeOut();
                /*setTimeout(function(){
                 if(fun){
                 fun();
                 }
                 },5000);*/
                if(fun){
                    fun();
                }
            });
        };
        var options = {
            debug:debug,//true显示报错
            /**
             * 发现有新版本时的方法
             * */
            findUpdateFunction:function(updateFunction,error,data){
                showUpdatePromptLayer('发现新版本，现在安装~',updateFunction);
            },
            /**
             * 切回app发现有新版本时的方法
             * */
            resumeFindUpdateFunction:function(updateFunction,error,data){
                showSpecialUpdatePromptLayer("发现新版本，现在安装",updateFunction);
            },
            /**
             * 发现有需要安装版本时执行的方法
             * */
            findInstallFunction:function(installFunction,error,data){
                showUpdatePromptLayer('发现可安装新版本,现在安装~',installFunction);
            },
            /**
             * 加载或安装失败时执行的方法
             * */
            installFailFunction:function(againFunction,error,data){
                showSpecialUpdatePromptLayer('安装失败,重试~',againFunction);
            },
            /**
             * 断网重试弹窗
             * */
            loadingFailedFunction:function(againFunction,error,data){
                showSpecialUpdatePromptLayer('安装失败,请检测网络后重试~',againFunction);
            },
            /**
             * 重启app弹窗
             * */
            restartAppFunction:function(againFunction,error,data){
                showUpdatePromptLayer('加载失败，请重新启动app~',againFunction);
                againFunction();
            }
        };
        cordovaHcpSetJS.init(options,function(){
            bossJS.init();
            console.log('程序初始化函数完成');
            //清空软件图标的新消息数
//            window.plugins.jPushPlugin.setApplicationIconBadgeNumber(0);
        });
    }
    /**
     * ——————————————————————————————————————————————————
     * */
</script>
<script>
    bossJS.showPage($("#bankCardPage"));
</script>
</body>
</html>